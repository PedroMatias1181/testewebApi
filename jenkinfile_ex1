pipeline{
	agent any
	stages{
		stage('Checkout'){
			steps{
				checkout scm
			}
		}
		stage('Restore'){
			steps{
				sh 'dotnet restore'
				echo 'packages restores Successfully'
			}
		}
		stage('Build'){
			steps{
				sh 'dotnet build --Configuration Release'
				echo 'Build completed'
			}
		}
		stage('Publish'){
			steps{
				sh 'dotnet publish'
			}
		}
	}
}